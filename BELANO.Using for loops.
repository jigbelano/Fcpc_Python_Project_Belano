print ("Multiplication Table \ n")

print ("Multiplication Table by 2")
for x in range (2,12,2):
    print (x)
print ("Multiplication Table by 3")
for x in range (3,18,3):
    print (x)
print ("Multiplication Table by 4")
for x in range (4,24,4):
    print (x)
print ("Multiplication Table by 12")
for x in range (12,132,12):
    print (x)

print ("Multiplication Table By 10 to 20 \ n")

#nested loop sample
for g in range (10,21):
    for p in range (1,11):
        print (g * p, end = '\ t')
    print ()
