# String
pangalan = "Ang pangalan ko ay Jerome Bravo"
kotse = "Ang gusto kong kotse ay Honda Civic 2018"
girlfriend = "Ang girlfriend ay si Arvi Dela Cruz"
aso = "Ang pangalan ng aso namin ay Lheo Robin Cruz"

# Numerical value
edad = "18"
halaga_ng_kotse = "1700000"
edad_ng_aso = "18"

# Arithmetic
addition = 10 + 100
subtraction = 10 - 5
multiplication = 5 * 5
division = 10 / 2
modulus = 10 % 4

print(pangalan)
print(kotse)
print(girlfriend)
print(aso)
print(edad)
print(halaga_ng_kotse)
print(edad_ng_aso)
print(addition)
print(subtraction)
print(multiplication)
print(division)
print(modulus)

if addition > subtraction:
    print("Addition is greater than subtraction")
elif multiplication > division:
    print("Multiplication is greater than divsion")
else:
    print("Wala sa nabanggit")
